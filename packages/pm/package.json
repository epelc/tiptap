  "files": [
    "src",
    "dist"
  ],
  "peerDependencies": {
    "@tiptap/core": "^2.0.0-beta.193"
  },
  "dependencies": {
    "prosemirror-changeset": "^2.2.0",
    "prosemirror-collab": "^1.3.0",
    "prosemirror-commands": "^1.3.1",
