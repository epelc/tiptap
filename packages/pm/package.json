  "repository": {
    "type": "git",
    "url": "https://github.com/ueberdosis/tiptap",
    "directory": "packages/pm"
  },
  "scripts": {
    "build": "tsup"
  },
  "tsup": {
    "entry": [
      "src/changeset/index.ts",
      "src/collab/index.ts",
      "src/commands/index.ts",
      "src/dropcursor/index.ts",
      "src/gapcursor/index.ts",
      "src/history/index.ts",
      "src/inputrules/index.ts",
      "src/keymap/index.ts",
      "src/markdown/index.ts",
      "src/menu/index.ts",
      "src/model/index.ts",
      "src/schema-basic/index.ts",
      "src/schema-list/index.ts",
      "src/state/index.ts",
      "src/tables/index.ts",
      "src/trailing-node/index.ts",
      "src/transform/index.ts",
      "src/view/index.ts"
    ],
    "dts": true,
    "splitting": true,
    "format": [
      "esm",
      "cjs"
    ]
  }
}
