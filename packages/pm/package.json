{
  "name": "@tiptap/pm",
  "description": "prosemirror wrapper package for tiptap",
  "version": "2.0.0-beta.209",
  "homepage": "https://tiptap.dev",
  "keywords": [
    "tiptap",
    "prosemirror"
  ],
  "license": "MIT",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/ueberdosis"
